{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "jsx": "react",
    "paths": {
      "@/": [
        "./"
      ]
    }
  },
  "include": [
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.js",
    "./**/*.jsx",
    "./.expo/types/**/*.ts",
    "./expo-env.d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
